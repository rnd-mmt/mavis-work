<?xml version="1.0" encoding="utf-8"?>

<odoo>

   <record id="nursing_department" model="hr.department">
      <field name="name">Nurse</field>
      <field name="department_type">nurse</field>
   </record>

	<data noupdate="0">
		<record id="sequence_round_number" model="ir.sequence">
   			<field name="name">acs.nurse.ward.round</field>
   			<field name="code">acs.nurse.ward.round</field>
   			<field name="active">TRUE</field>
   			<field name="prefix">RN</field>
   			<field name="padding">4</field>
   			<field name="number_next">1</field>
   			<field name="number_increment">1</field>
		</record>
	</data>

	<data noupdate="0">
		<record id="sequence_soins_infirmier" model="ir.sequence">
   			<field name="name">Soins infirmier</field>
   			<field name="code">acs.soins.infirmier</field>
   			<field name="prefix">SINF %(year)s/</field>
   			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>
	</data>
   
</odoo>