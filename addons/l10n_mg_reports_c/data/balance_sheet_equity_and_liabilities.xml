      <field name="sequence" eval="5"/>
      <field name="level" eval="2"/>
      <field name="style_overwrite">2</field>
      <field name="type">report_mg_cp_p</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_psv_tcp" model="account.financial.report">
      <field name="name">TOTAL CAPITAUX PROPRES</field>
      <field name="code">TCP_PSV_BSEL_MG</field>
      <field name="parent_id" ref="account_financial_report_l10n_mg_bsel_psv"/>
      <field name="formulas">balance = CE_CP_PSV_BSEL_MG.balance + PRC_CP_PSV_BSEL_MG.balance + EEVL_CP_PSV_BSEL_MG.balance + EEQU_CP_PSV_BSEL_MG.balance+RNPG_CP_PSV_BSEL_MG.balance+ACPRN_CP_PSV_BSEL_MG.balance</field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="1"/>
      <field name="style_overwrite">0</field>
      <field name="type">report_mg_cp_p</field>
    </record>
