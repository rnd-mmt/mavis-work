    <!-- BALANCE SHEET EQUITY AND LIABILITIES -->
    <record id="account_financial_report_l10n_mg_bsel" model="account.financial.report">
      <field name="name">Bilan Capitaux propres et Passifs</field>
      <field name="type_financial_report_mg">balance_cp_p</field>
      <field name="sign">-1</field>
      <field name="type">sum_mg</field>
      <field name="is_hidden" eval="True"/>
      <field name="style_overwrite">0</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_psv" model="account.financial.report">
      <field name="name">CAPITAUX PROPRES ET PASSIFS</field>
      <field name="parent_id" ref="account_financial_report_l10n_mg_bsel"/>
      <field name="sequence" eval="0"/>
      <field name="level" eval="1"/>
      <field name="style_overwrite">0</field>
      <field name="is_hidden">True</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_psv_cp" model="account.financial.report">
      <field name="name">CAPITAUX PROPRES</field>
      <field name="parent_id" ref="account_financial_report_l10n_mg_bsel_psv"/>
      <field name="sequence" eval="0"/>
      <field name="level" eval="1"/>
      <field name="style_overwrite">0</field>
      <field name="is_hidden">True</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_psv_cp_ce" model="account.financial.report">
      <field name="name">Capital émis</field>
      <field name="code">CE_CP_PSV_BSEL_MG</field>
      <field name="parent_id" ref="account_financial_report_l10n_mg_bsel_psv_cp"/>
      <field name="formulas">balance = -sum.balance</field>
      <field name="domain"
             eval="['|','|',('code', '=like', '101%'),('code', '=like', '108%'),('code', '=like', '109%')]"/>
      <field name="sequence" eval="0"/>
      <field name="level" eval="2"/>
      <field name="style_overwrite">2</field>
      <field name="type">report_mg_cp_p</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_psv_cp_prc" model="account.financial.report">
      <field name="name">Primes et réserves consolidées</field>
