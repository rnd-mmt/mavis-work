      <field name="formulas">balance = sum.credit</field>
      <field name="domain" eval="[('code', '=like', '48%')]"/>
      <field name="sequence" eval="2"/>
      <field name="level" eval="3"/>
      <field name="style_overwrite">3</field>
      <field name="type">report_mg_cp_p</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_psv_pc_ctdb" model="account.financial.report">
      <field name="name">Compte de trésorerie (découverts bancaires)</field>
      <field name="code">CTDB_PC_PSV_BSEL_MG</field>
      <field name="parent_id" ref="account_financial_report_l10n_mg_bsel_psv_pc"/>
      <field name="formulas">balance = -sum.balance</field>
      <field name="domain" eval="[('code', '=like', '519%')]"/>
      <field name="sequence" eval="4"/>
      <field name="level" eval="2"/>
      <field name="style_overwrite">2</field>
      <field name="type">report_mg_cp_p</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_psv_tpc" model="account.financial.report">
      <field name="name">TOTAL PASSIFS COURANTS</field>
      <field name="code">TPC_PSV_BSEL_MG</field>
      <field name="parent_id" ref="account_financial_report_l10n_mg_bsel_psv"/>
      <field name="formulas">balance = DCTPCTDLT_PC_PSV_BSEL_MG.balance + FCR_PC_PSV_BSEL_MG.balance +
        PPCAPC_PC_PSV_BSEL_MG.balance + AD_PC_PSV_BSEL_MG.balance + CTDB_PC_PSV_BSEL_MG.balance
      </field>
      <field name="sequence" eval="5"/>
      <field name="level" eval="1"/>
      <field name="style_overwrite">0</field>
      <field name="type">report_mg_cp_p</field>
    </record>
    <record id="account_financial_report_l10n_mg_bsel_tpsv" model="account.financial.report">
      <field name="name">TOTAL CAPITAUX PROPRES ET PASSIFS</field>
      <field name="code">TPSV_BSEL_MG</field>
      <field name="parent_id" ref="account_financial_report_l10n_mg_bsel"/>
      <field name="financial_report_id" ref="account_financial_report_l10n_mg_bsel"/>
