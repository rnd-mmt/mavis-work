      <field name="style_overwrite">2</field>
    </record>
    <record id="report_commissions_courtages" model="account.financial.report">
      <field name="name">Commissions et courtages</field>
      <field name="code">PRODAACC</field>
      <field name="parent_id" ref='report_activite_annexe'/>
