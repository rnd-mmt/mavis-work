      <field name="groupby">account_id</field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="3"/>
      <field name="style_overwrite">3</field>
      <field name="type">product_details</field>
    </record>
    <record id="report_frais_accesories" model="account.financial.report">
