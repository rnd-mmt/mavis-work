<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data noupdate="0">
    <!-- détails produits -->
    <record id="report_produits_details" model="account.financial.report">
      <field name="name">Détails produits</field>
      <field name="type_financial_report_mg">product_details</field>
    </record>

    <record id="report_produit_ordinaire" model="account.financial.report">
      <field name="name">TOTAL DES PRODUITS ORDINAIRES</field>
      <field name="code">PRODORD</field>
      <field name="financial_report_id" ref="report_produits_details"/>
      <field name="parent_id" ref="report_produits_details"/>
      <field name="formulas">balance = RPROPV.balance + CAFF.balance + PRODSD.balance + PRODIMM.balance + SUBEX.balance
        + APO.balance + PRODFI.balance
      </field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="0"/>
      <field name="type">product_details</field>
      <field name="style_overwrite">0</field>
    </record>
    <record id="report_chiffre_d_affaire" model="account.financial.report">
      <field name="name">CHIFFRES D'AFFAIRES</field>
      <field name="code">CAFF</field>
      <field name="parent_id" ref="report_produit_ordinaire"/>
      <field name="formulas">balance = PRODAACC.balance + PRODAAFAR.balance + VPSERV.balance + VPRODPROF.balance +
        VPRODF.balance + VAM.balance
      </field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="1"/>
      <field name="type">product_details</field>
      <field name="style_overwrite">1</field>
    </record>
    <record id="report_vente_net_rabais" model="account.financial.report">
      <field name="name">Ventes nets des Rabais remises ristournes Accordés</field>
      <field name="code">VNRRRA</field>
      <field name="parent_id" ref='report_chiffre_d_affaire'/>
      <field name="sequence" eval="1"/>
      <field name="level" eval="2"/>
      <field name="type">product_details</field>
      <field name="style_overwrite">2</field>
    </record>
    <record id="report_vente_produit_locaux" model="account.financial.report">
      <field name="name">Vente produits locaux</field>
      <field name="code">VPRODF</field>
      <field name="parent_id" ref='report_vente_net_rabais'/>
      <field name="domain" eval="[('code', '=like', '701%')]"/>
      <field name="formulas">balance = -sum.balance</field>
      <field name="groupby">account_id</field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="3"/>
      <field name="type">product_details</field>
      <field name="style_overwrite">3</field>
    </record>
    <record id="report_vente_produit_intermediaire" model="account.financial.report">
      <field name="name">Vente produits intermédiaires</field>
      <field name="code">VPRODPROF</field>
      <field name="parent_id" ref='report_vente_net_rabais'/>
      <field name="domain"
             eval="['|','|','|','|','|','|','|','|','|',('code', '=like', '702%'),('code', '=like', '703%'),('code', '=like', '704%'),('code', '=like', '705%'),('code', '=like', '7070000%'),('code', '=like', '7072000%'),('code', '=like', '7073000%'),('code', '=like', '7074000%'),('code', '=like', '7075000%'),('code', '=like', '7076000%')]"/>
      <field name="formulas">balance = -sum.balance</field>
      <field name="groupby">account_id</field>
      <field name="sequence" eval="2"/>
      <field name="level" eval="3"/>
      <field name="type">product_details</field>
      <field name="style_overwrite">3</field>
    </record>
    <record id="report_vente_prestation_service" model="account.financial.report">
      <field name="name">Vente prestations de service</field>
      <field name="code">VPSERV</field>
      <field name="parent_id" ref='report_vente_net_rabais'/>
      <field name="domain" eval="[('code', '=like', '706%')]"/>
      <field name="formulas">balance = -sum.balance</field>
      <field name="groupby">account_id</field>
      <field name="sequence" eval="3"/>
      <field name="level" eval="3"/>
      <field name="type">product_details</field>
      <field name="style_overwrite">3</field>
    </record>
    <record id="report_vente_autres_marchandises" model="account.financial.report">
      <field name="name">Autres ventes de marchandises</field>
      <field name="code">VAM</field>
      <field name="parent_id" ref='report_vente_net_rabais'/>
      <field name="domain" eval="[('code', '=like', '7071000%')]"/>
      <field name="formulas">balance = -sum.balance</field>
      <field name="groupby">account_id</field>
      <field name="sequence" eval="4"/>
      <field name="level" eval="3"/>
      <field name="type">product_details</field>
      <field name="style_overwrite">3</field>
    </record>

    <record id="report_activite_annexe" model="account.financial.report">
