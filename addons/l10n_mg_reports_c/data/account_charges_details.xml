    <record id="report_achat_consomme" model="account.financial.report">
      <field name="name">ACHATS CONSOMMES</field>
      <field name="code">ACONS</field>
      <field name="financial_report_id" ref="report_details_charges"/>
      <field name="parent_id" ref="report_details_charges"/>
      <field name="formulas">balance = AMFC.balance + VDS.balance + AEPS.balance + ANSMF.balance + AMAR.balance +
        FASA.balance
      </field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="1"/>
      <field name="type">charge_details</field>
      <field name="style_overwrite">0</field>
    </record>
    <record id="report_achat_matieres" model="account.financial.report">
      <field name="name">Achats de matières et fournitures consommables</field>
      <field name="code">AMFC</field>
      <field name="parent_id" ref='report_achat_consomme'/>
      <field name="domain" eval="['|',('code', '=like', '601%'),('code', '=like', '602%')]"/>
      <field name="formulas">balance = sum.balance</field>
      <field name="groupby">account_id</field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="2"/>
      <field name="type">charge_details</field>
      <field name="style_overwrite">1</field>
    </record>
