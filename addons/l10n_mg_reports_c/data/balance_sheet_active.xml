      <field name="code">EA_ANC_ACTV_BS_MG</field>
      <field name="parent_id" ref='account_financial_report_balance_sheet_actv_anc'/>
      <field name="formulas">brut = sum.balance ; amort = -sum.balance</field>
      <field name="domain">brut = ['|',('code', '=like', '206%'),('code', '=like', '207%')] ; amort = ['|',('code',
        '=like', '2906%'),('code', '=like', '2807%')]
