    <record id="account_financial_report_balance_sheet_actv_ac_tet" model="account.financial.report">
      <field name="name">Trésorerie et équivalents de trésorerie</field>
      <field name="code">TET_AC_ACTV_BS_MG</field>
      <field name="parent_id" ref='account_financial_report_balance_sheet_actv_ac'/>
      <field name="formulas">brut = PAET_CEA_AC_ACTV_BS_MG.brut + TFCDV_CEA_AC_ACTV_BS_MG.brut ; amort =
        PAET_CEA_AC_ACTV_BS_MG.amort + TFCDV_CEA_AC_ACTV_BS_MG.amort
      </field>
      <field name="sequence" eval="6"/>
      <field name="level" eval="2"/>
      <field name="style_overwrite">2</field>
    </record>

    <record id="account_financial_report_balance_sheet_actv_ac_tet_paet" model="account.financial.report">
      <field name="name">Placements et autres équivalents de trésorerie</field>
      <field name="code">PAET_CEA_AC_ACTV_BS_MG</field>
      <field name="parent_id" ref='account_financial_report_balance_sheet_actv_ac_tet'/>
      <field name="formulas">brut = sum.balance ; amort = -sum.balance</field>
      <field name="domain">brut = [('code', '=like', '50%')] ; amort = [('code', '=like', '59%')]</field>
      <field name="sequence" eval="0"/>
      <field name="level" eval="3"/>
      <field name="style_overwrite">3</field>
    </record>

    <record id="account_financial_report_balance_sheet_actv_ac_tet_tfcdv" model="account.financial.report">
      <field name="name">Trésorerie (fonds en caisse et dépôts à vue)</field>
      <field name="code">TFCDV_CEA_AC_ACTV_BS_MG</field>
      <field name="parent_id" ref='account_financial_report_balance_sheet_actv_ac_tet'/>
      <field name="formulas">brut = sum.balance</field>
      <field name="domain">brut = ['|','|','|','|','|','|','|',('code', '=like', '511%'),('code', '=like',
        '512%'),('code', '=like', '515%'),('code', '=like', '517%'),('code', '=like', '5187%'),('code', '=like',
        '52%'),('code', '=like', '53%'),('code', '=like', '54%')]
      </field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="3"/>
      <field name="style_overwrite">3</field>
    </record>

    <record id="account_financial_report_balance_sheet_actv_tac" model="account.financial.report">
      <field name="name">TOTAL ACTIFS COURANTS</field>
      <field name="code">TAC_ACTV_BS_MG</field>
      <field name="parent_id" ref="account_financial_report_balance_sheet_actv"/>
      <field name="formulas">brut = STKECR_AC_ACTV_BS_MG.brut + CEA_AC_ACTV_BS_MG.brut + TET_AC_ACTV_BS_MG.brut ; amort
        = STKECR_AC_ACTV_BS_MG.amort + CEA_AC_ACTV_BS_MG.amort + TET_AC_ACTV_BS_MG.amort
      </field>
      <field name="sequence" eval="3"/>
      <field name="level" eval="1"/>
      <field name="style_overwrite">1</field>
    </record>

    <record id="account_financial_report_balance_sheet_tactv" model="account.financial.report">
      <field name="name">TOTAL ACTIFS</field>
      <field name="code">TACTV_BS_MG</field>
      <field name="parent_id" ref="account_financial_report_balance_sheet_asset"/>
      <field name="formulas">brut = TANC_ACTV_BS_MG.brut + TAC_ACTV_BS_MG.brut ; amort = TANC_ACTV_BS_MG.amort +
        TAC_ACTV_BS_MG.amort
      </field>
      <field name="sequence" eval="4"/>
      <field name="level" eval="1"/>
      <field name="style_overwrite">1</field>
    </record>
