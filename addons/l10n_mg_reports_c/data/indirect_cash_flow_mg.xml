<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data noupdate="0">
    <!-- Flux de trésorerie méthode indirecte -->
    <record id="report_flux_tresorerie_indirecte" model="account.financial.report">
      <field name="name">Trésorerie indirecte</field>
      <field name='type_financial_report_mg'>cash_flow</field>
      <field name='note_mg' eval="True"/>
    </record>
    <record id="report_flux_lies_activites" model="account.financial.report">
      <field name="name">A - FLUX DE TRESORERIE LIES A L'ACTIVITE</field>
      <field name="code">FTLA</field>
      <field name="financial_report_id" ref="report_flux_tresorerie_indirecte"/>
      <field name="parent_id" ref="report_flux_tresorerie_indirecte"/>
      <field name="formulas">balance = MPVALC.balance + VARFAD.balance + VARCAC.balance + VARST.balance + VIMPD.balance
        + VPPCA.balance + AMPR.balance + FLUXRNET.balance
      </field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="1"/>
      <field name="type">report_mg_cf</field>
      <field name="style_overwrite">0</field>
    </record>
    <record id="flux_report_resultat_net_exo" model="account.financial.report">
      <field name="name">RESULTAT NET DE L'EXERCICE</field>
      <field name="code">FLUXRNET</field>
      <field name="parent_id" ref='report_flux_lies_activites'/>
      <field name="domain" eval="['|',('code', '=like', '120%'),('code', '=like', '129%')]"/>
      <field name="formulas">balance = sum.balance</field>
      <field name="groupby">account_id</field>
      <field name="sequence" eval="1"/>
      <field name="level" eval="2"/>
      <field name="type">report_mg_cf</field>
      <field name="style_overwrite">1</field>
    </record>
<!--    <record id="flux_report_resultat_net_exo" model="account.financial.report">-->
<!--      <field name="name">RESULTAT NET DE L'EXERCICE</field>-->
<!--      <field name="code">FLUXRNET</field>-->
<!--      <field name="parent_id" ref='report_flux_lies_activites'/>-->
<!--&lt;!&ndash;      <field name="domain" eval="['|',('code', '=like', '120%'),('code', '=like', '129%')]"/>&ndash;&gt;-->
