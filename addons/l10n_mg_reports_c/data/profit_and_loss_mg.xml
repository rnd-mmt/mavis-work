    <field name="name">Compte de résultats</field>
    <field name='type_financial_report_mg'>income_statement</field>
    <field name='note_mg' eval="True"/>
    <field name="sign">-1</field>
    <field name="type">sum_mg</field>
  </record>

  <record id="account_financial_report_line_01_mg_cdr_pde" model="account.financial.report">
    <field name="name">I- Production de l'excercice</field>
    <field name="code">MGCDR_PDE</field>
    <field name="financial_report_id" ref="account_financial_report_l10n_mg_cdr"/>
    <field name="parent_id" ref="account_financial_report_l10n_mg_cdr"/>
    <field name="sequence" eval="1"/>
    <field name="formulas">balance = PDE_CDA.balance + PDE_PS.balance + PDE_PI.balance</field>
    <field name="level" eval="1"/>
    <field name="type">report_mg_pl</field>
    <field name="style_overwrite">0</field>
  </record>

  <record id="account_financial_report_line_mg_pde_cda" model="account.financial.report">
    <field name="name">Chiffres d'affaires</field>
    <field name="code">PDE_CDA</field>
    <field name="sequence" eval="1"/>
    <field name="parent_id" ref='account_financial_report_line_01_mg_cdr_pde'/>
    <field name="domain" eval="[('code', '=like', '70%'),('code', 'not like', '709%')]"/>
    <field name="formulas">balance = -sum.balance + PDE_CDA_1.balance</field>
    <field name="level" eval="2"/>
    <field name="green_on_positive" eval="0"/>
