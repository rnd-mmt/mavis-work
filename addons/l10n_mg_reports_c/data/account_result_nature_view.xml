<odoo>
	<data noupdate="0">
		<!-- Compte de résultat par nature -->
		<record id="report_compte_resultat_nature" model="account.financial.report">
			<field name="name">Compte de résultat par nature</field>
			<field name='type_financial_report_mg'>income_statement</field>
			<field name="type">sum_mg</field>
		</record>
		<record id="report_production_exercice" model="account.financial.report">
			<field name="name">I - PRODUCTION DE L'EXERCICE</field>
			<field name="code">PE</field>
			<field name="financial_report_id" ref="report_compte_resultat_nature" />
			<field name="parent_id" ref="report_compte_resultat_nature" />
			<field name="formulas">balance = CANATURE.balance + PRODIMMN.balance + PSN.balance</field>
			<field name="sequence" eval="1" />
			<field name="level" eval="1" />
			<field name="type">report_mg_pl</field>
			<field name="style_overwrite">0</field>
		</record>
		<record id="report_nature_chiffre_affaire" model="account.financial.report">
			<field name="name">Chiffre d'affaire</field>
			<field name="code">CANATURE</field>
			<field name="parent_id" ref='report_production_exercice'/>
			<field name="domain" eval="[('code', '=like', '70%')]"/>
			<field name="formulas">balance = -sum.balance</field>
			<field name="groupby">account_id</field>
			<field name="sequence" eval="1" />
			<field name="level" eval="2" />
			<field name="type">report_mg_pl</field>
			<field name="style_overwrite">1</field>
		</record>
		<record id="report_nature_production_stockee" model="account.financial.report">
			<field name="name">Production stockée</field>
			<field name="code">PSN</field>
			<field name="parent_id" ref='report_production_exercice' />
			<field name="domain" eval="[('code','=like','71%')]" />
<!--			<field name="domain" eval="['|',('code','=like','713%'),('code','=like','714%')]" />-->
			<field name="formulas">balance = -sum.balance</field>
			<field name="groupby">account_id</field>
			<field name="sequence" eval="2" />
			<field name="level" eval="2" />
			<field name="type">report_mg_pl</field>
			<field name="style_overwrite">1</field>
		</record>
		<record id="report_nature_production_immobilisee" model="account.financial.report">
			<field name="name">Production immobilisée</field>
			<field name="code">PRODIMMN</field>
			<field name="parent_id" ref='report_production_exercice' />
			<field name="formulas">balance = PRODIMM.balance</field>
			<field name="groupby">account_id</field>
			<field name="sequence" eval="3" />
			<field name="level" eval="2" />
			<field name="type">report_mg_pl</field>
			<field name="style_overwrite">1</field>
		</record>
		<record id="report_nature_conso_exercice" model="account.financial.report">
			<field name="name">II - CONSOMMATION DE L'EXERCICE</field>
			<field name="code">CEN</field>
			<field name="financial_report_id" ref="report_compte_resultat_nature" />
			<field name="parent_id" ref="report_compte_resultat_nature" />
			<field name="formulas">balance = NATUREACONS.balance + NATURESEAC.balance</field>
			<field name="sequence" eval="2" />
			<field name="level" eval="1" />
			<field name="type">report_mg_pl</field>
			<field name="style_overwrite">0</field>
		</record>
