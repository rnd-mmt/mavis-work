<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Commission provider -->
        <record id="seq_coprest" model="ir.sequence">
            <field name="name">Commission Prestataire</field>
            <field name="code">commission.prestataire</field>
            <field name="prefix">PREST</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_coprest_sheet" model="ir.sequence">
            <field name="name">Résumé feuille de commission prestataire</field>
            <field name="code">commission.prestataire.sheet</field>
            <field name="prefix">CPS</field>
            <field name="padding">3</field>
        </record>

        <record id="coprest_product" model="product.product">
            <field name="name">Commission prestataire</field>
            <field name="type">service</field>
            <field name="hospital_product_type">os</field>
            <field name="list_price">0.0</field>
            <field name="standard_price">0.0</field>
        </record>

        <!-- Company Data -->
        <record id="base.main_company" model="res.company" >
            <field name="coprest_product_id" ref="coprest_product"/>
        </record>

    </data>
</odoo>
